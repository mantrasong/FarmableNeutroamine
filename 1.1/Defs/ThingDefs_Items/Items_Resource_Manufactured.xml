<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef Name="MakeNeutroamine">
	<defName>MakeNeutroamine</defName>
		<label>Make Neutroamine</label>
		<description>Makes neutroamine, a base ingredient for other drugs.</description>
		<jobString>Making Neutroamine.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Neutroaleaves</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Neutroaleaves</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Neutroamine>1</Neutroamine>
		</products>
		<workAmount>250</workAmount>
		<workSkill>Crafting</workSkill>
		<recipeUsers>
			<li>DrugLab</li>
		</recipeUsers>
		<researchPrerequisite>NeutroaleafRefining</researchPrerequisite>
	</RecipeDef>
	
